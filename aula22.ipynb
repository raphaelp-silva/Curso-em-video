   "source": [
    "def fatorial(n):\n",
    "    f=1\n",
    "    for c in range(1, n+1):\n",
    "        f *= c\n",
    "    return f\n",
    "\n",
    "\n",
    "num = int(input(\"Digite um valor\"))\n",
    "fat = fatorial(num)\n",
    "print(f\"O fatorial de {num} é {fat}\")"
   ]
  }
